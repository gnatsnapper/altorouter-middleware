{
    "name": "gnatsnapper/altorouter-middleware",
    "description": "A PSR-15 Middleware to allow the use of the venerable AltoRouter in Middleware-based PHP applications",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "keywords": [
        "middleware",
        "psr",
        "psr-15"
    ],

    "autoload": {
        "psr-4": {
    "Gnatsnapper\\Middleware\\": "src"
    }
    },
    "require": {
        "php" : "^7.3",
        "altorouter/altorouter": "^2.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.12",
        "laminas/laminas-diactoros": "^2.3",
        "laminas/laminas-stratigility": "^3.2"
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
